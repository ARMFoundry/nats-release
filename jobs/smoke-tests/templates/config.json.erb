<%
  nats_ips = nil
  if_p("nats.machines") { |ips| nats_ips = ips }
  unless nats_ips
    nats_ips = link("nats").instances.map(&:address)
  end
%>



{
  "Hosts": [ <%= nats_ips.map{|ip| "\"#{ip}\""}.join(', ') %> ],
  "User": "<%= nats.user %>",
  "Password": "<%= nats.password %>",
  "Port": <%= nats.port %>
}
